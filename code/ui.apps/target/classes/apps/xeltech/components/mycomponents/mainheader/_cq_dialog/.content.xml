<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Edit Menu"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.title.v2.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu1"
                                                name="./menu1"/>
                                            <Copy_x0020_of_x0020_title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu2"
                                                name="./menu2"/>
                                            <Copy_x0020_of_x0020_Copy_x0020_of_x0020_title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu3"
                                                name="./menu3"/>
                                            <Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu4"
                                                name="./menu4"/>
                                            <Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu5"
                                                name="./menu5"/>
                                            <Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_Copy_x0020_of_x0020_title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Leave empty to use the page title."
                                                fieldLabel="Menu6"
                                                name="./menu6"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
